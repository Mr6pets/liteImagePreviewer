{
  "name": "liteImagePreviewer",
  "version": "1.0.0",
  "description": "轻便版图片浏览器",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run sass:watch\" \"npm run start\"",
    "sass:watch": "sass --watch src/scss:public/css",
    "sass:build": "sass src/scss:public/css --style compressed",
    "build": "npm run sass:build",
    "dist": "node build.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "archiver": "^7.0.1",
    "clean-css": "^5.3.3",
    "concurrently": "^8.2.2",
    "sass": "^1.69.5",
    "terser": "^5.43.1"
  },
  "keywords": [
    "image",
    "previewer",
    "viewer",
    "gallery",
    "web"
  ],
  "author": "Mr6pets",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  }
}
