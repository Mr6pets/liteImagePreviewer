{
  "name": "liteImagePreviewer",
  "version": "1.0.0",
  "description": "轻便版图片浏览器",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run sass:watch\" \"npm run start\"",
    "sass:watch": "sass --watch src/scss:public/css",
    "sass:build": "sass src/scss:public/css --style compressed",
    "build": "npm run sass:build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "sass": "^1.69.5",
    "concurrently": "^8.2.2"
  },
  "keywords": ["image", "previewer", "viewer", "gallery", "web"],
  "author": "Mr6pets",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  }
}
